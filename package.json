{
  "name": "@boxicons/svelte",
  "version": "1.0.0",
  "type": "module",
  "description": "Svelte components for Boxicons",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/icons/*.d.ts",
      "svelte": "./dist/icons/*.js",
      "default": "./dist/icons/*.js"
    }
  },
  "sideEffects": false,
  "files": ["dist"],
  "scripts": {
    "generate": "node scripts/generate-icons.cjs",
    "build": "npm run generate && svelte-package -i src -o dist",
    "clean": "rm -rf dist src/icons",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:package": "npm publish"
  },
  "peerDependencies": {
    "svelte": ">=4.0.0"
  },
  "devDependencies": {
    "@boxicons/core": "file:../boxicons-core",
    "@sveltejs/package": "^2.2.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.0",
    "typescript": "^5.3.0"
  },
  "keywords": ["boxicons", "icons", "svelte", "svelte-icons", "svg"],
  "author": "Boxicons",
  "homepage": "https://boxicons.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/box-icons/boxicons-svelte"
  }
}
